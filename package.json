{
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:android": "expo build:android",
    "build:ios": "expo build:ios",
    "eject": "expo eject",
    "test": "node ./node_modules/jest/bin/jest",
    "test:dev": "node ./node_modules/jest/bin/jest --watch",
    "test:update": "node ./node_modules/jest/bin/jest --updateSnapshot"
  },
  "dependencies": {
    "@feathersjs/authentication-client": "^4.5.8",
    "@feathersjs/feathers": "^4.5.8",
    "@feathersjs/rest-client": "^4.5.8",
    "@feathersjs/socketio-client": "^4.5.8",
    "@react-native-community/masked-view": "0.1.10",
    "@react-navigation/native": "^5.7.3",
    "@react-navigation/stack": "^5.9.0",
    "expo": "~38.0.8",
    "expo-barcode-scanner": "~8.2.1",
    "expo-status-bar": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "~16.11.0",
    "react-dom": "~16.11.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-38.0.2.tar.gz",
    "react-native-flash-message": "^0.1.16",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-maps": "0.27.1",
    "react-native-reanimated": "~1.9.0",
    "react-native-safe-area-context": "~3.0.7",
    "react-native-screens": "~2.9.0",
    "react-native-side-menu": "^1.1.3",
    "react-native-web": "~0.11.7",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.8.6",
    "@react-native-community/async-storage": "^1.12.0",
    "babel-preset-expo": "~8.1.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.3",
    "jest": "26.4.2",
    "jest-enzyme": "^7.1.2",
    "jest-expo": "38.0.2",
    "jest-prop-type-error": "^1.1.0",
    "jest-stare": "^2.0.1",
    "react-test-renderer": "^16.13.1",
    "redux-mock-store": "^1.5.4"
  },
  "jest": {
    "preset": "jest-expo",
    "transform": {
      "^.+\\.[jt]sx?$": "babel-jest"
    },
    "reporters": [
      "default",
      "jest-stare"
    ],
    "setupFilesAfterEnv": [
      "./jest/setup-tests.js"
    ],
    "setupFiles": [
      "jest-prop-type-error",
      "./jest/jest-mocks.js",
      "./jest/test-utils.js"
    ],
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|@sentry/.*)"
    ],
    "silent": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/coverage/**",
      "!**/index.js",
      "!**/jest-stare/**",
      "!**/jest/**",
      "!**/node_modules/**",
      "!**/babel.config.js",
      "!**/*.styles.js",
      "!**/*.definitions.js"
    ],
    "watchPathIgnorePatterns": [
      "<rootDir>/jest-stare"
    ]
  },
  "private": true
}
